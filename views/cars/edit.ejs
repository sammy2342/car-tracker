<%- include('../partials/header') %> 

<h1>Edit Car</h1>
<form action="/cars/<%= car._id %>?_method=PUT" method="POST" autocomplete="off">
    <div>
        <input type="text" name="name" value="<%=car.name%>">
    </div>
    <div>
        <input type="text" name="model" value="<%= car.model %>">
    </div>
    <div>
        <input type="date" name="year" value="<%= car.year %>">
    </div>
    <div>
        <input type="number" name="price" value="<%= car.price %>">
    </div>
    <div>
        <input type="number" name="mileage" value="<%= car.mileage %>">
    </div>
    <button type="submit">Edit Car</button>
</form>

<%- include('../partials/footer') %> 